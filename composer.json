{
	"name": "DCAF/DCAF-Social",
	"description": "Social aggregation framework",
	"type": "project",
	"keywords": ["DCAF","DCAF-Social"],
	"homepage": "http://www.dcaf-social.com/",
	"license": "proprietary",
	"authors": [
		{
			"name": "Alexander Rosenberg",
			"homepage": "http://www.myjumbledweb.com/",
			"role": "developer"
		}, {
			"name": "Shashank Sanjay",
			"homepage": "http://www.shashanksanjay.com/",
			"role": "developer"
		}
	],
	"support": {
		"email": "support@dcaf-social.com",
		"issues": "https://github.com/ShashankSanjay/DCAF-Social/issues",
		"wiki": "https://github.com/ShashankSanjay/DCAF-Social/wiki"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "AlchemyAPI/alchemyapi_php",
				"version": "dev-master",
				"source": {
					"url": "https://github.com/AlchemyAPI/alchemyapi_php",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	],
	"require": {
		"AlchemyAPI/alchemyapi_php": "dev-master",
		"laravel/framework": "4.1.16",
		"twbs/bootstrap":  "3.0.0",
		"zizaco/confide": "3.1.0",
		"zizaco/entrust": "1.1.0",
		"jasonlewis/basset": "dev-master",
		"robclancy/presenter": "1.2.*",
		"j20/php-uuid": "dev-master",
		"bllim/datatables": "*",
		"thomaswelton/laravel-oauth": "0.1.0.*@dev",
		"facebook/php-sdk" : "*@stable",
		"liebig/cron": "dev-master",
		"AlchemyApi/alchemyapi_php": "dev-master"
	},
	"require-dev": {
		"way/generators": "dev-master",
		"mockery/mockery": "dev-master@dev",
		"summerstreet/woodling": "0.1.*",
		"barryvdh/laravel-ide-helper": "dev-master",
		"leafo/lessphp": "v0.4.0",
		"natxet/CssMin": "dev-master",
		"lmammino/jsmin4assetic": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/contracts",
			"app/controllers",
			"app/facades",
			"app/library",
			"app/models",
			"app/observers",
			"app/presenters",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/library",
			"vendor/AlchemyAPI/alchemyapi_php/alchemyapi.php"
		],
		"files": []
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
